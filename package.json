{
	"name": "vscode-nodebook",
	"displayName": "Nodebook",
	"version": "0.0.1",
	"private": false,
	"description": "Node.js Notebook for VS Code",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandNodebook#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandNodebook/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandNodebook.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"updatetypes": "cd src/types && vscode-dts dev && vscode-dts master"
	},
	"contributes": {
		"commands": [
			{
				"command": "nodebook.toggleDebugging",
				"icon": "$(bug)",
				"title": "Toggle Debugging"
			},
			{
				"command": "nodebook.restartKernel",
				"icon": "$(refresh)",
				"title": "Restart Kernel"
			}
		],
		"languages": [
			{
				"aliases": [
					"Node.js Notebook"
				],
				"id": "nodebook"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "nodebook.toggleDebugging",
					"group": "navigation@1",
					"when": "notebookEditorFocused"
				},
				{
					"command": "nodebook.restartKernel",
					"group": "navigation@2",
					"when": "notebookEditorFocused"
				}
			]
		},
		"notebookProvider": [
			{
				"displayName": "Node.js Notebook",
				"selector": [
					{
						"filenamePattern": "*.nodebook"
					}
				],
				"viewType": "nodebook"
			}
		]
	},
	"activationEvents": [
		"onNotebookEditor:nodebook",
		"onNotebook:nodebook",
		"onDebugDynamicConfigurations:node"
	],
	"dependencies": {
		"get-port": "7.1.0",
		"rimraf": "5.0.5"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/glob": "8.1.0",
		"@types/node": "20.12.10",
		"glob": "10.3.12"
	},
	"publishConfig": {
		"access": "public"
	},
	"enableProposedApi": true
}
